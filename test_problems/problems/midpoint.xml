    <answer><![CDATA[#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main(void)
{
	int x1, y1, x2, y2;
	double midx, midy;

	printf("Enter x1:\n");
	scanf("%d", &x1);

	printf("Enter y1:\n");
	scanf("%d", &y1);

	printf("Enter x2:\n");
	scanf("%d", &x2);

	printf("Enter y2:\n");
	scanf("%d", &y2);

	midx = (x1 + x2) / 2.0;
	midy = (y1 + y2) / 2.0;

	printf("Midpoint: (%.1f, %.1f)", midx, midy);

	return 0;
}
]]></answer>
