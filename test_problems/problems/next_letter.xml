    <answer><![CDATA[#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main(void)
{
	char input;
	char result;

	printf("Enter letter:\n");
	scanf("%c", &input);

	result = (char)(input + 1);

	printf("%c -> %c", input, result);

	return 0;
}
]]></answer>
