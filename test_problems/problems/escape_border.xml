    <answer><![CDATA[#include <stdio.h>

int main(void)
{
	printf("  /\\-----/\\\n");
	printf(" /  \\---/  \\\n");
	printf("< Lab Seven >\n");
	printf(" \\  /---\\  /\n");
	printf("  \\/-----\\/\n");

	return 0;
}
]]></answer>
